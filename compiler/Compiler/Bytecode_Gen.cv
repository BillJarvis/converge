		else:
			return_failed_label := self._create_label()
			self._instructions.append(self._target::Instr_Add_Failure_Frame.new(node.src_info, return_failed_label))
			self._nesting.append([NEST_NORMAL])
			self.preorder(node.return_expr)
			self._nesting.del(-1)
			self._instructions.append(self._target::Instr_Return.new(node.src_info))

			self._set_label(return_failed_label)
			self._instructions.append(self._target::Instr_Builtin_Lookup.new(node.src_info, _BUILTIN_FAIL_OBJ))
			self._instructions.append(self._target::Instr_Return.new(node.src_info))




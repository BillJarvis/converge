	func _t_ibreak(node):

		i := self._nesting.len() - 1
		while not [NEST_FOR, NEST_WHILE].find(self._nesting[i][0]):
			if [NEST_CLASS, NEST_FUNC].find(self._nesting[i][0]):
				self._compiler.error("Trying to break whilst not in a 'while' or 'for' loop", node.src_info)
			self._instructions.append(self._target::Instr_Remove_Failure_Frame.new(node.src_info))
			i -= 1
		
		if self._nesting[i][0] == NEST_WHILE:
			self._instructions.append(self._target::Instr_Branch.new(node.src_info, self._nesting[i][2]))
		elif self._nesting[i][0] == NEST_FOR:
			self._instructions.append(self._target::Instr_Branch.new(node.src_info, self._nesting[i][1]))
		else:
			raise "XXX"



	func _t_icontinue(node):

		i := self._nesting.len() - 1
		while not [NEST_FOR, NEST_WHILE].find(self._nesting[i][0]):
			if [NEST_CLASS, NEST_FUNC].find(self._nesting[i][0]):
				self._compiler.error("Trying to continue whilst not in a 'while' or 'for' loop", node.src_info)
			self._instructions.append(self._target::Instr_Remove_Failure_Frame.new(node.src_info))
			i -= 1
		
		if self._nesting[i][0] == NEST_WHILE:
			self._instructions.append(self._target::Instr_Remove_Failure_Frame.new(node.src_info))
			self._instructions.append(self._target::Instr_Branch.new(node.src_info, self._nesting[i][1]))
		elif self._nesting[i][0] == NEST_FOR:
			self._instructions.append(self._target::Instr_Remove_Failure_Frame.new(node.src_info))
			self._instructions.append(self._target::Instr_Fail_Now.new(node.src_info))



	func _t_iconjunction(node):

		for i := 0.to(node.exprs.len()):
			next_expr_label := self._create_label()
			self.preorder(node.exprs[i])
			if i + 1 == node.exprs.len():
				self._instructions.append(self._target::Instr_Dup.new(node.src_info))
			self._instructions.append(self._target::Instr_Branch_If_Not_Fail.new(node.src_info, next_expr_label))
			self._instructions.append(self._target::Instr_Fail_Now.new(node.src_info))
			self._set_label(next_expr_label)



	func _t_ialternation(node):

		end_of_alternation_label := self._create_label()
		for expr := node.exprs.iterate():
			next_expr_label := self._create_label()
			self._nesting.append([NEST_NORMAL])
			self._instructions.append(self._target::Instr_Add_Failure_Frame.new(node.src_info, next_expr_label))
			self.preorder(expr)
			self._instructions.append(self._target::Instr_EYield.new(node.src_info))
			self._nesting.del(-1)
			self._instructions.append(self._target::Instr_Branch.new(node.src_info, end_of_alternation_label))
			self._set_label(next_expr_label)
		self._instructions.append(self._target::Instr_Fail_Now.new(node.src_info))
		
		self._set_label(end_of_alternation_label)



	func _t_iassignment(node):
	
		if node.type != ITree::STD_ASSIGN:
			for target := node.targets.iterate():
				if ITree::IVar.instantiated(target):
					self._add_lookup_var(target.var_name, target.src_info)
				elif ITree::ISlot_Lookup.instantiated(target):
					self.preorder(target)
				elif ITree::IGet.instantiated(target):
					self.preorder(target)
				else:
					raise "XXX"
	
		self.preorder(node.expr)
		
		if node.targets.len() > 1:
			self._instructions.append(self._target::Instr_Unpack_Assign.new(node.src_info, node.targets.len()))
		
		for target := node.targets.iterate():
			if node.type == ITree::ADD_ASSIGN:
				self._instructions.append(self._target::Instr_Add.new(node.src_info))
			elif node.type == ITree::SUB_ASSIGN:
				self._instructions.append(self._target::Instr_Sub.new(node.src_info))

			if ITree::IVar.instantiated(target):
				self._add_assign_to_var(target.var_name, target.src_info)
			elif ITree::ISlot_Lookup.instantiated(target):
				self.preorder(target.object_expr)
				self._instructions.append(self._target::Instr_Assign_Slot.new(target.src_info, target.slot_name))
			elif ITree::IGet.instantiated(target):
				self.preorder(target.get_object)
				self._instructions.append(self._target::Instr_Pre_Apply_Slot_Lookup.new(target.src_info, "set"))
				self.preorder(target.index)
				self._instructions.append(self._target::Instr_Pull.new(target.src_info, 2))
				self._instructions.append(self._target::Instr_Apply.new(target.src_info, 2))
			else:
				raise "XXX"

			if node.targets.len() > 1:
				self._instructions.append(self._target::Instr_Pop.new(node.src_info))



	func _t_inot(node):
	
		success_label := self._create_label()
		self._nesting.append([NEST_NORMAL])
		self._instructions.append(self._target::Instr_Add_Failure_Frame.new(node.src_info, success_label))
		self.preorder(node.expr)
		self._instructions.append(self._target::Instr_Branch_If_Fail.new(node.src_info, success_label))
		self._instructions.append(self._target::Instr_Remove_Failure_Frame.new(node.src_info))
		self._nesting.del(-1)
		self._instructions.append(self._target::Instr_Fail_Now.new(node.src_info))
		
		self._set_label(success_label)
		self._instructions.append(self._target::Instr_Builtin_Lookup.new(node.src_info, _BUILTIN_NULL_OBJ))


	
	func _t_ibinary(node):

		self.preorder(node.lhs)
		
		if node.type == ITree::BINARY_ADD:
			slot_name := "+"
		elif node.type == ITree::BINARY_SUB:
			slot_name := "-"
		elif node.type == ITree::BINARY_MUL:
			slot_name := "*"
		elif node.type == ITree::BINARY_DIV:
			slot_name := "/"
		elif node.type == ITree::BINARY_MOD:
			slot_name := "%"

		self._instructions.append(self._target::Instr_Pre_Apply_Slot_Lookup.new(node.src_info, slot_name))
		self.preorder(node.rhs)
		self._instructions.append(self._target::Instr_Apply.new(node.src_info, 1))



	func _t_icomparison(node):

		self.preorder(node.lhs)
		self.preorder(node.rhs)
		if node.type == ITree::IS_COMPARISON:
			self._instructions.append(self._target::Instr_Is.new(node.src_info))
		elif node.type == ITree::EQ_COMPARISON:
			self._instructions.append(self._target::Instr_EQ.new(node.src_info))
		elif node.type == ITree::NEQ_COMPARISON:
			self._instructions.append(self._target::Instr_NEQ.new(node.src_info))
		elif node.type == ITree::LEQ_COMPARISON:
			self._instructions.append(self._target::Instr_LEQ.new(node.src_info))
		elif node.type == ITree::GEQ_COMPARISON:
			self._instructions.append(self._target::Instr_GEQ.new(node.src_info))
		elif node.type == ITree::LE_COMPARISON:
			self._instructions.append(self._target::Instr_LE.new(node.src_info))
		else:
			self._instructions.append(self._target::Instr_GE.new(node.src_info))




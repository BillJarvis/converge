	func _t_iapplication(node):
	
		if ITree::ISlot_Lookup.instantiated(node.func_expr):
			self.preorder(node.func_expr.object_expr)
			self._instructions.append(self._target::Instr_Pre_Apply_Slot_Lookup.new(node.func_expr.src_info, node.func_expr.slot_name))
		else:
			self.preorder(node.func_expr)
			
		for arg := node.args.iterate():
			self.preorder(arg)
			
		self._instructions.append(self._target::Instr_Apply.new(node.src_info, node.args.len()))



